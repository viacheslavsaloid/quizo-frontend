    "@ngxs/store": "^3.6.2",
    "@swimlane/ngx-datatable": "^17.0.0",
    "jwt-decode": "^2.2.0",
    "moment": "^2.29.1",
    "ng-dynamic-component": "^6.1.0",
    "ngx-mat-select-search": "^2.2.0",
    "ngx-material-file-input": "^2.1.1",
    "ngx-material-timepicker": "^5.5.3",
    "ngx-progressbar": "^6.0.3",
    "ngx-toastr": "^12.1.0",
    "ngx-translate-multi-http-loader": "^3.0.0",
    "rollbar": "^2.17.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
